  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./_",
    "prepush": "npm run lint",
    "lint": "eslint --cache --ext .js,.jsx ./src/",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx ./src/",
    "build:site": "webpack --config ./src/webpack/site/webpack.config.js",
    "build:prototype": "webpack --config ./src/webpack/prototype/webpack.config.js",
    "server:site": "node ./src/webpack/dev-server/index.js --site",
    "server:prototype": "node ./src/webpack/dev-server/index.js --prototype",
    "start": "npm run server:prototype",
    "http-server": "echo open http://127.0.0.1:8080 && http-server ./_/prototype -s -p 8080"
  },
